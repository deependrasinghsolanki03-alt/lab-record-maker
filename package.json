{
  "name": "lab-record-maker",
  "displayName": "Lab Record Maker",
  "description": "Appends C++ code to DOCX",
  "publisher": "DeependraSinghSolanki",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/deependrasinghsolanki03-alt/lab-record-maker"
},
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.appendCodeToRecord"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.appendCodeToRecord",
        "title": "Append Code to Lab Record"
      },
      {
        "command": "extension.editStudentDetails",
        "title": "Edit Student Details (Lab Record)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "docx": "^9.5.1"
  }
}
